<p id="notice"><%= notice %></p>

<h1>Listing Viewmenus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @viewmenus.each do |viewmenu| %>
      <tr>
        <td><%= viewmenu.name %></td>
        <td><%= viewmenu.description %></td>
        <td><%= link_to 'Show', viewmenu %></td>
        <td><%= link_to 'Edit', edit_viewmenu_path(viewmenu) %></td>
        <td><%= link_to 'Destroy', viewmenu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Viewmenu', new_viewmenu_path %>
