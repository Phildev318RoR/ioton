<p id="notice"><%= notice %></p>

<h1>Listing Permissionviewroles</h1>

<table>
  <thead>
    <tr>
      <th>Permissionview</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @permissionviewroles.each do |permissionviewrole| %>
      <tr>
        <td><%= permissionviewrole.permissionview_id %></td>
        <td><%= permissionviewrole.role_id %></td>
        <td><%= link_to 'Show', permissionviewrole %></td>
        <td><%= link_to 'Edit', edit_permissionviewrole_path(permissionviewrole) %></td>
        <td><%= link_to 'Destroy', permissionviewrole, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Permissionviewrole', new_permissionviewrole_path %>
