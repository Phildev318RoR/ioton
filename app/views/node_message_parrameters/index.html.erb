<p id="notice"><%= notice %></p>

<h1>Listing Node Message Parrameters</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Node message</th>
      <th>Value type</th>
      <th>Value length</th>
      <th>Value index</th>
      <th>Downloaded</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @node_message_parrameters.each do |node_message_parrameter| %>
      <tr>
        <td><%= node_message_parrameter.name %></td>
        <td><%= node_message_parrameter.node_message_id %></td>
        <td><%= node_message_parrameter.value_type_id %></td>
        <td><%= node_message_parrameter.value_length %></td>
        <td><%= node_message_parrameter.value_index %></td>
        <td><%= node_message_parrameter.downloaded %></td>
        <td><%= link_to 'Show', node_message_parrameter %></td>
        <td><%= link_to 'Edit', edit_node_message_parrameter_path(node_message_parrameter) %></td>
        <td><%= link_to 'Destroy', node_message_parrameter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Node message parrameter', new_node_message_parrameter_path %>
